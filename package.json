{
  "name": "good-books",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@styled-icons/material-outlined": "^10.23.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.21.1",
    "formik": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setuptests.ts"
    ],
    "globals": {
      "GOOGLE_API_BOOKS": "https: //www.googleapis.com/books/v1/volumes"
    },
    "moduleNameMapper": {
      "^@screens": "<rootDir>/src/screens",
      "^@components": "<rootDir>/src/components",
      "^@context": "<rootDir>/src/context",
      "^@services": "<rootDir>/src/services",
      "^@icons": "<rootDir>/src/icons"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    ".{ts,tsx,html}": [
      "prettier --write",
      "node ./node_modules/eslint/bin/eslint.js --cache --fix"
    ],
    "src/**/*.{ts,tsx,html}": [
      "prettier --write",
      "node ./node_modules/eslint/bin/eslint.js --cache --fix"
    ]
  },
  "devDependencies": {
    "@types/material-ui": "^0.21.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "axios-mock-adapter": "^1.19.0",
    "babel-preset-react-app": "^10.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "react-app-rewire-alias": "^0.1.9",
    "react-app-rewired": "^2.1.8"
  }
}
